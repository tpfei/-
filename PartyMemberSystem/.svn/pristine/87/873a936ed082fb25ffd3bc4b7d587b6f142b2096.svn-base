//对roleID进行验证
function check1() 
{ 
	 var check = false; 
	 var roleId = document.getElementById("check1").value; 
	 alert(roleId);
	 if (roleId.length > 4) 
	 { 
	  document.getElementById("checktext1").innerHTML = " × 内容不能为空，请重新输入"; 
	  check = false; 
	 } 
	 else 
	 { 
	  document.getElementById("checktext1").innerHTML = " √"; 
	  check = true; 
	 } 
	 return check; 
} 