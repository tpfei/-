<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>PartyMemberSystem</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>org.lanqiao.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>UserServlet</display-name>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>org.lanqiao.servlet.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/viewUser.do</url-pattern>
		<url-pattern>/addUser.do</url-pattern>
		<url-pattern>/editUser.do</url-pattern>
		<url-pattern>/deleteUser.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>BranchServlet</display-name>
		<servlet-name>BranchServlet</servlet-name>
		<servlet-class>org.lanqiao.servlet.BranchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BranchServlet</servlet-name>
		<url-pattern>/addBranch.do</url-pattern>
		<url-pattern>/saveBranch.do</url-pattern>
		<url-pattern>/deleteBranch.do</url-pattern>
		<url-pattern>/updateBranch.do</url-pattern>
		<url-pattern>/selectBranch.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>DangyuanServlet</display-name>
		<servlet-name>DangyuanServlet</servlet-name>
		<servlet-class>org.lanqiao.servlet.DangyuanServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DangyuanServlet</servlet-name>
		<url-pattern>/downDYMessage.do</url-pattern>
		<url-pattern>/viewDangyuan.do</url-pattern>
		<url-pattern>/deleteDangyuan.do</url-pattern>
		<url-pattern>/saveDangyuan.do</url-pattern>
		<url-pattern>/saveDangyuanForPOI.do</url-pattern>
		<url-pattern>/selectDangyuan.do</url-pattern>
		<url-pattern>/out.do</url-pattern>
		<url-pattern>/outDangyuan.do</url-pattern>
		<url-pattern>/deleteNewDeptName.do</url-pattern>
		<url-pattern>/chaxun.do</url-pattern>
		<url-pattern>/chakanDangyuan.do</url-pattern>
		    <url-pattern>/updateDangyuan.do</url-pattern>
		
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ExcelUpLoadServlet</display-name>
		<servlet-name>ExcelUpLoadServlet</servlet-name>
		<servlet-class>org.lanqiao.servlet.ExcelUpLoadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ExcelUpLoadServlet</servlet-name>
		<url-pattern>/import.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<description></description>
		<display-name>PayServlet</display-name>
		<servlet-name>PayServlet</servlet-name>
		<servlet-class>org.lanqiao.servlet.PayServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PayServlet</servlet-name>
		<url-pattern>/showPay.do</url-pattern>
		<url-pattern>/updatePay.do</url-pattern>
	</servlet-mapping>

</web-app>